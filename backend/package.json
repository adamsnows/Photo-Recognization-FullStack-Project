  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.6.0",
    "fastify": "^5.3.0",
    "fastify-type-provider-zod": "^4.0.2",
    "pg": "^8.14.1",
    "pino-http": "^10.4.0",
    "pino-pretty": "^13.0.0",
    "tsup": "^8.4.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "22.14.1",
    "prisma": "6.6.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  }
